{
  "expo": {
    "name": "bee-smart",
    "slug": "bee-smart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo.png"
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.anonymous.beesmart"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "expo-sqlite"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a93edd03-039d-4c55-8879-cb6762f2c236"
      }
    }
  }
}
